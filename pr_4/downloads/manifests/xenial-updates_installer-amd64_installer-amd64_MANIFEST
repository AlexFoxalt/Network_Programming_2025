cdrom/debian-cd_info.tar.gz     -- isolinux config files for CD
cdrom/initrd.gz                 -- initrd for use with isolinux to build a CD
cdrom/vmlinuz                   -- kernel for use with isolinux to build a CD
cdrom/xen/initrd.gz             -- initrd for installing under Xen
cdrom/xen/vmlinuz               -- kernel image for installing under Xen
cdrom/xen/xm-debian.cfg         -- example Xen configuration
hd-media/boot.img.gz            -- 800 mb image (compressed) for USB memory stick
hd-media/initrd.gz              -- for use on USB memory sticks
hd-media/vmlinuz                -- for use on USB memory sticks
hwe-cdrom/debian-cd_info.tar.gz -- isolinux config files for CD
hwe-cdrom/initrd.gz             -- initrd for use with isolinux to build a CD
hwe-cdrom/vmlinuz               -- kernel for use with isolinux to build a CD
hwe-cdrom/xen/initrd.gz         -- initrd for installing under Xen
hwe-cdrom/xen/vmlinuz           -- kernel image for installing under Xen
hwe-cdrom/xen/xm-debian.cfg     -- example Xen configuration
hwe-hd-media/boot.img.gz        -- 800 mb image (compressed) for USB memory stick
hwe-hd-media/initrd.gz          -- for use on USB memory sticks
hwe-hd-media/vmlinuz            -- for use on USB memory sticks
hwe-netboot/boot.img.gz         -- compressed network install image for USB memory stick
hwe-netboot/mini.iso            -- tiny CD image that boots the netboot installer
hwe-netboot/netboot.tar.gz      -- tarball of PXE boot directory
hwe-netboot/ubuntu-installer    -- PXE boot directory for tftp server
hwe-netboot/xen/initrd.gz       -- initrd for installing under Xen
hwe-netboot/xen/vmlinuz         -- kernel image for installing under Xen
hwe-netboot/xen/xm-debian.cfg   -- example Xen configuration
netboot/boot.img.gz             -- compressed network install image for USB memory stick
netboot/mini.iso                -- tiny CD image that boots the netboot installer
netboot/netboot.tar.gz          -- tarball of PXE boot directory
netboot/ubuntu-installer        -- PXE boot directory for tftp server
netboot/xen/initrd.gz           -- initrd for installing under Xen
netboot/xen/vmlinuz             -- kernel image for installing under Xen
netboot/xen/xm-debian.cfg       -- example Xen configuration
